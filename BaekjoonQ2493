import heapq
import sys

N = int(sys.stdin.readline())
tower = list(map(int, sys.stdin.readline().split()))
answer = [0]
stack = []

for i in range(1,N):
  temp = 0
  for j in range(i-1,-1,-1):
    if tower[j] >= tower[i]:
      answer.append(j+1)
      break
      
  if len(answer) == i:
    answer.append(0)



print(answer)



 




